<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="5dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="8dp">


        <TableRow
            android:id="@+id/user_identify_row"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/user_identify_image"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/user_ico_false" />
        </TableRow>

        <TableRow
            android:id="@+id/money_identify_row"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/money_identify_image"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/money_ico_false" />
        </TableRow>

        <TableRow
            android:id="@+id/marriage_identify_row"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/marriage_identify_image"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/marriage_ico_false" />
        </TableRow>

        <TableRow
            android:id="@+id/work_identify_row"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/work_identify_image"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/work_ico_false" />
        </TableRow>

        <TableRow
            android:id="@+id/education_identify_row"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/education_identify_image"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/education_ico_false" />

        </TableRow>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">


        <TableRow
            android:id="@+id/user_identify_row_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <TextView
                android:id="@+id/user_identify_text"
                android:gravity="center_horizontal"
                android:text="身份"
                android:textColor="@color/text_gray"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/money_identify_row_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <TextView
                android:id="@+id/money_identify_text"
                android:gravity="center_horizontal"
                android:text="资产"
                android:textColor="@color/text_gray"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/marriage_identify_row_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <TextView
                android:id="@+id/marrige_identify_text"
                android:gravity="center_horizontal"
                android:text="单身"
                android:textColor="@color/text_gray"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/work_identify_row_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <TextView
                android:id="@+id/work_identify_text"
                android:gravity="center_horizontal"
                android:text="职业"
                android:textColor="@color/text_gray"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/education_identify_row_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">


            <TextView
                android:id="@+id/education_identify_text"
                android:gravity="center_horizontal"
                android:text="学历"
                android:textColor="@color/text_gray"
                android:textSize="10dp" />


        </TableRow>


    </LinearLayout>
</LinearLayout>